  },
  "homepage": "https://github.com/stefan-feltmann/packing-tracking#readme",
  "devDependencies": {
    "@aws-cdk/assert": "1.116.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "aws-cdk": "1.116.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
